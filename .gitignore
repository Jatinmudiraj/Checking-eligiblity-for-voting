//Program for checking the eligiblity of the user for voting. For myCaptain project


#include<iostream>
using namespace std;

int main() {
    cout<< "This is the campaign for checking your eligiblity of voteing by myCaptain\n"; 

    int age;
    cout << "Enter Your age & we will tell you\nYou are eligible for voteing or not:-\n";
    cin>>age;

    if (age >= 18 && age < 100) {
        cout << "Superb You are eligible for voting.\n";
    }
    else if (age>100){
        cout << "You are too old but still eligible for voting";
    } 
    else {
        cout << "You are not eligible for voting.\n";
        cout << "You will be eligible for voting after " << 18-age <<" years.";
    }

    return 0;
    
}
